{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "bookshelf",
      "settings": {
        "client": "postgres",
        "database": "${process.env.POSTGRES_DB || 'myblog'}",
        "host": "blog_postgres",
        "port": "5432",
        "username": "${process.env.POSTGRES_USER || 'postgres'}",
        "password": "${process.env.POSTGRES_PASSWORD || 'my_password_123'}"
      },
      "options": {}
    }
  }
}
